{
    "name": "infogrambeg/efiskalizacija-php-sdk",
    "description": "PHP SDK za eFiskalizacija.cloud - fiskalizacija raÄuna u Srbiji (VSDC API)",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "fiskalizacija",
        "srbija",
        "vsdc",
        "efiskalizacija",
        "fiscalization",
        "invoice"
    ],
    "homepage": "https://efiskalizacija.cloud",
    "authors": [
        {
            "name": "Infogram BEG",
            "email": "info@efiskalizacija.cloud"
        }
    ],
    "require": {
        "php": ">=8.1",
        "ext-curl": "*",
        "ext-json": "*",
        "ext-hash": "*",
        "psr/log": "^1.0 || ^2.0 || ^3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0 || ^11.0",
        "phpstan/phpstan": "^1.10"
    },
    "autoload": {
        "psr-4": {
            "Efiskalizacija\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Efiskalizacija\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "scripts": {
        "test": "phpunit --testsuite Unit",
        "test:all": "phpunit"
    },
    "config": {
        "sort-packages": true
    }
}
